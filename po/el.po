#: userhelper.c:862
#, c-format
msgid "Password for %s: "
msgstr "Κωδικός για %s: "

#: userhelper.c:2348
#, c-format
msgid "userhelper must be setuid root\n"
msgstr "Το userhelper πρέπει να είναι setuid root\n"

