msgstr "Error: %s\n"

#: pam-panel-icon.c:450
#, c-format
msgid "Failed to run command \"%s\": %s\n"
msgstr "No s'ha pogut executar l'ordre \"%s\": %s\n"

#: pam-panel-icon.c:463
#, c-format
msgid "Failed to set IO channel nonblocking: %s\n"
msgstr "No s'ha pogut posar el canal d'E/S a no blocar: %s\n"

#: pam-panel-icon.c:542
msgid "pam-panel-icon: failed to connect to session manager\n"
msgstr "pam-panel-icon: ha fallat connectar al gestor de la sessió\n"

