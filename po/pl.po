#: ../pam-panel-icon.c:98
#, c-format
msgid "Failed to drop administrator privileges: %s"
msgstr "Rezygnacja z przywilejów administratora nie powiodła się: %s"

#: ../pam-panel-icon.c:114
#, c-format
msgid ""
"Failed to drop administrator privileges: pam_timestamp_check returned "
"failure code %d"
msgstr ""
"Rezygnacja z przywilejów administratora nie powiodła się: "
"pam_timestamp_check zwrócił kod błędu %d"

#: ../pam-panel-icon.c:170 ../pam-panel-icon.c:208
msgid "Keep Authorization"
msgstr "Zachowaj upoważnienie"

#: ../pam-panel-icon.c:180 ../pam-panel-icon.c:212
msgid "Forget Authorization"
msgstr "Zrezygnuj z upoważnienia"

#: ../pam-panel-icon.c:202
msgid ""
"You're currently authorized to configure system-wide settings (that affect "
"all users) without typing the administrator password again. You can give up "
"this authorization."
msgstr ""
"Jesteś aktualnie upoważniony w zakresie konfiguracji ustawień systemu (które "
"dotyczą wszystkich użytkowników) bez konieczności wprowadzania ponownie "
"hasła administratora. Można zrezygnować z tego upoważnienia."

#: ../pam-panel-icon.c:394
msgid "pam_timestamp_check is not setuid root"
msgstr "pam_timestamp_check nie jest setuid roota"

#: ../pam-panel-icon.c:399
msgid "no controlling tty for pam_timestamp_check"
msgstr "brak sterującego TTY dla pam_timestamp_check"

#: ../pam-panel-icon.c:403
msgid "user unknown to pam_timestamp_check"
msgstr "nieznany użytkownik dla pam_timestamp_check"

#: ../pam-panel-icon.c:407
msgid "permissions error in pam_timestamp_check"
msgstr "błąd uprawnień w pam_timestamp_check"

#: ../pam-panel-icon.c:411
msgid "invalid controlling tty in pam_timestamp_check"
msgstr "nieprawidłowy sterujący TTY dla pam_timestamp_check"

